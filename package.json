{
  "name": "onion-fetch",
  "version": "0.0.1",
  "description": "一个轻量且灵活的、受 Koa 洋葱模型启发的 fetch 客户端库，允许开发者轻松定义和组合请求与响应中间件，以实现强大且可定制的 HTTP 请求。",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "build": "unbuild",
    "dev": "unbuild --watch"
  },
  "keywords": [
    "fetch",
    "http",
    "request",
    "middleware",
    "onion-model",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "h3": "^1.15.3",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.1.3"
  }
}
