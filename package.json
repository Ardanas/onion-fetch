{
  "name": "onion-fetch",
  "version": "0.0.1",
  "description": "Onion Fetch is a lightweight, middleware-based fetch library for browsers and Node.js, inspired by Koa.js. It provides a flexible and extensible way to make HTTP requests.",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "build": "unbuild",
    "dev": "unbuild --watch"
  },
  "keywords": [
    "fetch",
    "http",
    "request",
    "middleware",
    "onion-model",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "h3": "^1.15.3",
    "listhen": "^1.9.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.1.3"
  }
}